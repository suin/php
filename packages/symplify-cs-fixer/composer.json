{
    "name": "suin/symplify-cs-fixer",
    "type": "library",
    "description": "Make Symplify Coding Standard's fixers work with PHP-CS-Fixer",
    "keywords": [
        "coding-standards",
        "coding-style",
        "php-cs-fixer",
        "static-analysis",
        "symplify-coding-standard"
    ],
    "homepage": "https://github.com/suin/php",
    "license": "MIT",
    "authors": [
        {
            "name": "suin",
            "email": "suinyeze@gmail.com",
            "homepage": "https://github.com/suin",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/suin/php/issues"
    },
    "require": {
        "php": ">=7.1",
        "friendsofphp/php-cs-fixer": "^3.2",
        "symfony/finder": "^5.3",
        "symfony/yaml": "^5.3",
        "symplify/coding-standard": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "SymplifyCsFixer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SymplifyCsFixer\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}